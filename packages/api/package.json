{
  "name": "@turbo-super/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "generate-api": "npx openapi-typescript-codegen --input https://dev-editor.superduperai.co/openapi.json --output src/superduperai/api --client axios --useOptions",
    "lint": "eslint . --config eslint.config.js"
  },
  "dependencies": {
    "@sobytes/openapi-typescript-codegen": "^0.25.0",
    "@turbo-super/core": "workspace:*",
    "axios": "^1.10.0",
    "form-data": "^4.0.3",
    "openapi": "1.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/compat": "^1.0.0",
    "typescript-eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0"
  },
  "peerDependencies": {
    "next": "^15.0.0"
  }
}
