  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:skip-migration": "next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe && pnpm run check:conflicts",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "check:conflicts": "grep -r '<<<<<<< \\|======= \\|>>>>>>> ' --include='*.ts' --include='*.tsx' --include='*.js' --include='*.jsx' --exclude-dir=node_modules --exclude-dir=.next --exclude-dir=.vercel . && echo 'Found Git conflict markers!' && exit 1 || echo 'No Git conflicts found'",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx src/lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "generate-api": "openapi --input https://dev-editor.superduperai.co/openapi.json --output lib/api --client axios --useOptions",
    "test:openapi-all": "tsx tests/run-all-openapi-tests.ts",
    "test:openapi-final": "tsx tests/openapi-integration-final-test.ts",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "test:video": "node tests/video-generation-smoke-test.js",
    "test:video:dry": "node tests/video-generation-real-test.js",
    "test:video:live": "node tests/video-generation-real-test.js --live",
    "test:video:i2v": "node tests/video-generation-image-to-video-test.js",
    "test:websocket": "node tests/websocket-debug-test.js",
    "test:image": "node tests/image-generation-debug-test.js",
    "test:project-ws": "node tests/project-websocket-test.js",
    "test:simple": "node tests/simple-image-test.js",
    "test:no-project": "node tests/simple-no-project-test.js",
    "test:project-image": "node tests/project-image-endpoint-test.js",
    "test:final": "node tests/final-generate-image-test.js"
  },
