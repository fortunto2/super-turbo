{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "prebuild": "node scripts/generate-llms.js",
    "build": "pnpm run prebuild && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "postbuild": "node scripts/generate-markdown.js",
    "typecheck": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && tsc --noEmit",
    "generate-types": "pnpm exec ts-node scripts/generate-translation-types.ts",
    "check-env": "node scripts/check-env.js",
    "preview": "NODE_OPTIONS='--no-warnings' pnpm dlx @opennextjs/cloudflare build && pnpm dlx @opennextjs/cloudflare preview",
    "deploy": "NODE_OPTIONS='--no-warnings' pnpm dlx @opennextjs/cloudflare build && pnpm dlx @opennextjs/cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "clean-cache": "rimraf .next/cache",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@ai-sdk/azure": "^1.3.23",
    "@formatjs/intl-localematcher": "^0.6.1",
    "@next/mdx": "^15.3.1",
    "@next/third-parties": "^15.3.1",
    "@opentelemetry/api-logs": "^0.52.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-logs": "^0.52.0",
    "@opentelemetry/sdk-metrics": "^1.25.0",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@turbo-super/api": "workspace:*",
    "@turbo-super/core": "workspace:*",
    "@turbo-super/features": "workspace:*",
    "@turbo-super/payment": "workspace:*",
    "@turbo-super/shared": "workspace:*",
    "@turbo-super/ui": "workspace:*",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/otel": "^1.13.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "contentlayer2": "^0.5.8",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.4",
    "langsmith": "^0.3.39",
    "lucide-react": "^0.446.0",
    "mdx-bundler": "^10.1.1",
    "minimatch": "^10.0.3",
    "negotiator": "^1.0.0",
    "next": "15.3.1",
    "next-contentlayer2": "^0.5.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.6.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/compat": "^1.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@opennextjs/cloudflare": "1.0.0-beta.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@turbo-super/eslint-config": "workspace:*",
    "@turbo-super/tailwind": "workspace:*",
    "@types/mdx": "^2.0.13",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.17.32",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@typescript-eslint/typescript-estree": "^8.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "jsdom": "^24.0.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "tailwindcss": "3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^1.3.1",
    "wrangler": "^4.14.1"
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}